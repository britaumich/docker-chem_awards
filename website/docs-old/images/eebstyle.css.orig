th.rotate {
  /* Something you can count on */
  height: 160px;
  white-space: nowrap;
  vertical-align: bottom;
}

th.rotate > div {
  transform:
    /* Magic Numbers */
  /*   translate(25px, 51px)
 *       /* 45 is really 360 - 45 */
    /* rotate(315deg);  */
    rotate(270deg);
   width: 30px;
}

th.rotate > div > span {
  border-bottom: 1px solid #ccc;
  padding: 6px 3px;
  padding-bottom: 9px;
}

/* These aren't needed and will be weird if transforms don't work */
.csstransforms & th.rotate {
  height: 140px;
  white-space: nowrap;
}

